{
  "protected require": {
    "prefix": "preq",
    "body": [
      "local ok, $2 = pcall(require, '$1')",
      "if not ok then",
      "\treturn $0",
      "end"
    ]
  },
  "stylua-ignore": {
    "prefix": "sti",
    "body": "-- stylua: ignore"
  },
  "stylua-ignore-block": {
    "prefix": "stib",
    "body": "-- stylua: ignore start $1\n$0\n-- stylua: ignore end"
  }
}
