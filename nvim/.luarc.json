{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua",
    "lua/?.lua",
    "lua/?/init.lua",
    "?.lua",
    "?/init.lua"
  ],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME/lua",
    "${3rd}/luv/library"
  ],
  "globals"
  "hint.enable": true,
  "hint.setType": true,
  "hint.arrayIndex": "Disable",
  "diagnostics.properties.globals": [
    "vim"
  ],
  "diagnostics.disable": [
    "missing-fields"
  ],
  "telemetry.enable": false
}
